FN_GET_FULL	FUNCTION	64	   IF i > 0

FN_GET_FULL	FUNCTION	65	   THEN

FN_GET_FULL	FUNCTION	66	      RETURN 0;

FN_GET_FULL	FUNCTION	67	   END IF;

FN_GET_FULL	FUNCTION	68	

