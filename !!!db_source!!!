TR_TMC_UPD_1	TRIGGER	26	

TR_TMC_UPD_1	TRIGGER	27	   DELETE FROM tmc

TR_TMC_UPD_1	TRIGGER	28	         WHERE parent = :new.id;

TR_TMC_UPD_1	TRIGGER	29	

TR_TMC_UPD_1	TRIGGER	30	   COMMIT;

TR_TMC_UPD_1	TRIGGER	31	

