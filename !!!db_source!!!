PR_GET_UM_DATA	PROCEDURE	47	         SELECT * FROM um_TRANSIT;

PR_GET_UM_DATA	PROCEDURE	48	

PR_GET_UM_DATA	PROCEDURE	49	      COMMIT;

PR_GET_UM_DATA	PROCEDURE	50	   END IF;

