REFRESH_MS_REP_SPEC_FACTS	PROCEDURE	61	        VALUES ('MS_REP_SPEC_FACTS total records: ' || c, 'MS_REP_SPEC_FACTS');

REFRESH_MS_REP_SPEC_FACTS	PROCEDURE	62	

