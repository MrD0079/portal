PR_GET_UM_DATA	PROCEDURE	41	               WHERE um_date = a.um_date;

PR_GET_UM_DATA	PROCEDURE	42	

PR_GET_UM_DATA	PROCEDURE	43	         COMMIT;

PR_GET_UM_DATA	PROCEDURE	44	      END LOOP;

PR_GET_UM_DATA	PROCEDURE	45	

