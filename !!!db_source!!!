PR_MERCH_SPEC_BODY_INS_NEW	PROCEDURE	41	                   sort_,

PR_MERCH_SPEC_BODY_INS_NEW	PROCEDURE	42	                   art_,

PR_MERCH_SPEC_BODY_INS_NEW	PROCEDURE	43	                   name_,

