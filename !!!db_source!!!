TR_AC_ACCEPT_NEW	TRIGGER	79	               '    '

TR_AC_ACCEPT_NEW	TRIGGER	80	            || :new.ac_id

TR_AC_ACCEPT_NEW	TRIGGER	81	            --|| '  : '

TR_AC_ACCEPT_NEW	TRIGGER	82	            --|| a.head

TR_AC_ACCEPT_NEW	TRIGGER	83	            || '  '

