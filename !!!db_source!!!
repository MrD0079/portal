PR_GET_ACT_DATA	PROCEDURE	22	

PR_GET_ACT_DATA	PROCEDURE	23	   SELECT COUNT (*) INTO c FROM p_activity_transit;

PR_GET_ACT_DATA	PROCEDURE	24	

