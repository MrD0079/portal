TR_AC_ACCEPT_NEW	TRIGGER	63	                    ac.vac1,

TR_AC_ACCEPT_NEW	TRIGGER	64	                    ac.vac2,

TR_AC_ACCEPT_NEW	TRIGGER	65	                    ac.vac3

TR_AC_ACCEPT_NEW	TRIGGER	66	               FROM ac, user_list u, user_list u1

