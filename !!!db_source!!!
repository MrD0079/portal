FN_HAS_PARENT_DB	FUNCTION	14	                              FROM assist

FN_HAS_PARENT_DB	FUNCTION	15	                             WHERE child = tn_)

FN_HAS_PARENT_DB	FUNCTION	16	               OR slave = tn_)

