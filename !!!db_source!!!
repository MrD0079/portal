PR_GET_ACT_DATA	PROCEDURE	14	             'A2014_12_Activity',

PR_GET_ACT_DATA	PROCEDURE	15	             'p_activity_transit',

PR_GET_ACT_DATA	PROCEDURE	16	             '"USER_FIO","USER_INN","FIO_ETA","Activity_Date","Activity_Name","TP_QTY","H_LINK","Country"',

