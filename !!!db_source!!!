SDU_LAST_UPDATE	TRIGGER	7	BEGIN

SDU_LAST_UPDATE	TRIGGER	8	   :NEW.lu := SYSDATE;

SDU_LAST_UPDATE	TRIGGER	9	END sdu_last_update; 

